{
    "name": "voult/api-client",
    "description": "APIs client implementing PSR 7 specification.",
    "version": "1.0",
    "time": "2019-10-16",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Edmund Pacha",
            "email": "voults@gmail.com",
            "homepage": "http://voult.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-curl": "*",
        "lib-curl": "*",
        "psr/http-message": "*",
        "fig/http-message-util": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5",
        "ramsey/uuid": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Voult\\ApiClient\\": "src/Voult/ApiClient/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Voult\\ApiClient\\Tests\\": "src/Voult/ApiClient/Tests"
        }
    }
}
